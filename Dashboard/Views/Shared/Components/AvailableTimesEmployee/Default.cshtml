@model ICollection<Core.Availability>

<ul>
    @if (Model.Count > 0)
    {
        @foreach (var item in Model.Where(x => x.AvailableFrom > DateTime.Now))
        {
            <li>From: @item.AvailableFrom</li>
            <li>To: @item.AvailableTo</li>
            <hr/>
        }
    }
    else
    {
        <p>This employee is not available</p>
    }
</ul>